{
  "name": "a2r",
  "version": "0.3.64",
  "description": "A2R Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf ./dist;tsc",
    "test": "jest",
    "test-dev": "npx jest --watch",
    "upload": "ts-node-dev ./src/tools/increaseVersion.ts;npm run build;npm publish;",
    "debug": "ts-node-dev --debug --respawn --transpileOnly ./src/services/commands/index.ts",
    "help": "ts-node-dev --respawn --transpileOnly ./src/services/commands/index.ts --frameworkLogLevel verbose -h",
    "start": "node ./dist/index.js",
    "initEmptyProject": "rm -rf ../examples/empty;mkdir ../examples/empty;cd ../examples/empty;npm init --force;npm install a2r@0.3.64 -s;npx a2r --frameworkLogLevel verbose --init;",
    "initEmptyAndStartProject": "rm -rf ../examples/empty;mkdir ../examples/empty;cd ../examples/empty;npm init --force;npm install a2r@0.3.64 --save;npx a2r --frameworkLogLevel verbose --init;npx a2r --dev  --frameworkLogLevel verbose",
    "initFromScratch": "rm -rf ../examples/empty;mkdir ../examples/empty;cd ../examples/empty;npm install a2r@0.3.64 -s;npx a2r --frameworkLogLevel verbose --init;",
    "uploadAndInitFromScratch": "npm run upload;ts-node-dev ./src/tools/waitForCurrentVersion.ts;npm run initFromScratch;",
    "uploadAndInstallGlobally": "ts-node-dev ./src/tools/increaseVersion.ts;npm run build;npm publish;sleep 10s;npm i -g a2r",
    "uploadAndStartProject": "npm run upload;ts-node-dev ./src/tools/waitForCurrentVersion.ts;npm run initEmptyAndStartProject;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acttoreact/A2R.git"
  },
  "keywords": [
    "a2r",
    "binpar",
    "framework",
    "reactjs",
    "react",
    "nextjs",
    "reactive",
    "websocket",
    "redis"
  ],
  "author": "Ignacio Ferro Pic√≥n <nacho@binpar.com> (https://a2r.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acttoreact/A2R/issues"
  },
  "homepage": "https://a2r.com",
  "bin": {
    "a2r": "./dist/services/commands/index.js"
  },
  "dependencies": {
    "chokidar": "^3.0.2",
    "colors": "^1.3.3",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "express": "^4.17.1",
    "get-port": "^5.0.0",
    "next": "^9.1.0",
    "open": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^3.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "triple-beam": "^1.3.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/rimraf": "^2.0.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/triple-beam": "^1.3.0",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.40"
  }
}
